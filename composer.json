    "keywords": ["activecollab", "api", "sdk"],
    "homepage": "https://github.com/activecollab/activecollab-feather-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@gmail.com"
        }
    ],
    "require": {
      "php" : ">=5.6.0",
      "ext-curl" : "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\SDK\\" : "src"
        }
    }
}
